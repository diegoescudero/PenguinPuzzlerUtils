<?xml version="1.0" encoding="UTF-8"?>

<!-- 
Keywords:
	level - Enclosing tag for the entire level
		width - Width in number of grid squares
		height - Height in number of grid squares
	block - Basic ice block
		x - horizontal location in grid squares
		y - vertical location in grid squares (0 is the top of the screen)
	start - Starting point for main character
		x - horizontal location in grid squares
		y - vertical location in grid squares (0 is the top of the screen)

NOTE: Attributes must be in the order shown to be processed correctly!
	 -->

<level
    width="8"
    height="11" >
    
    <block x="5" y="0"/>
    <block x="0" y="1"/>
    <block x="7" y="1"/>
    <block x="3" y="2"/>
    <block x="6" y="5"/>
    <block x="7" y="6"/>
    <block x="1" y="7"/>
    <block x="7" y="9"/>
    <block x="2" y="10"/>
    
    <pipe x="5" y="3" dir="UP"/>
    <pipe x="3" y="9" dir="RIGHT"/>
    <pipe x="6" y="8" dir="UP"/>
    
    <ramp x="2" y="4" dir="UP"/>
    <ramp x="4" y="1" dir="RIGHT"/>
    <ramp x="3" y="7" dir="UP"/>
    
    <breakable x="3" y="5"/>
	<breakable x="4" y="4"/>
    <breakable x="5" y="5"/>
	<breakable x="4" y="6"/>
	
    <start x="2" y="1"/>
	<finish x="4" y="5"/>
</level>